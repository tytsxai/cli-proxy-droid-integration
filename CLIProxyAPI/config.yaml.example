port: 8318
remote-management:
  allow-remote: false
  secret-key: ""
auth-dir: "~/.cli-proxy-api"
debug: true
logging-to-file: false
usage-statistics-enabled: true
proxy-url: ""
request-retry: 3
quota-exceeded:
  switch-project: true
  switch-preview-model: true
auth:
  providers: []
generative-language-api-key: []

# Third-party API Configuration
# Note: base-url is the API root, proxy will append /v1/responses automatically
codex-api-key:
  - api-key: "YOUR_API_KEY_HERE"
    prefix: "provider"
    base-url: "https://your-provider.example.com/api"
